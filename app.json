{
  "expo": {
    "name": "MyTodoApp",
    "slug": "todo-app-1",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/todo-app-logo.png",
    "scheme": "todoapp1",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sub27.todoapp1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/todo-app-logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.sub27.todoapp1"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/todo-app-logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9fc7bb88-2421-4844-a3d1-05ef28330708"
      }
    }
  }
}